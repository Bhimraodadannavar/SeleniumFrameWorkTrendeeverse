<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
  <test thread-count="3" parallel="classes"  name="chromeTestRunner" >
  <parameter name="browser" value="chrome">
  <classes>
      <class name="com.ecommerce.myaddress.AddressModuleTest"/>
      <class name="com.ecommerce.myaddress.CartTest"/>
      <class name="com.ecommerce.myaddress.WishListTest"/>
      <class name="com.ecommerce.myaddress.MerchantLoginTest"/>
      </classes>
  </parameter>
  </test> <!-- Test -->
  <test thread-count="3" parallel="classes"  name="firefoxTestRunner" >
  <parameter name="browser" value="firefox">
  <classes>
      <class name="com.ecommerce.myaddress.AddressModuleTest"/>
      <class name="com.ecommerce.myaddress.CartTest"/>
      <class name="com.ecommerce.myaddress.WishListTest"/>
      <class name="com.ecommerce.myaddress.MerchantLoginTest"/>
      </classes>
  </parameter>
  </test> <!-- test-->
</suite><!-- suite-->
